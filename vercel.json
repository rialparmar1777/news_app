{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "routes": [
    {
      "src": "/flutter.js",
      "dest": "/public/flutter.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/public/main.dart.js"
    },
    {
      "src": "/flutter_service_worker.js",
      "dest": "/public/flutter_service_worker.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/public/icons/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/public/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "API_KEY": "7911212b47fd4131aeb437134c3f9c36",
    "API_BASE_URL": "https://newsapi.org/v2"
  }
} 