{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/flutter.js",
      "dest": "/flutter.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/main.dart.js"
    },
    {
      "src": "/flutter_service_worker.js",
      "dest": "/flutter_service_worker.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 