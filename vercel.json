{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/flutter.js",
      "dest": "/public/flutter.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/public/main.dart.js"
    },
    {
      "src": "/flutter_service_worker.js",
      "dest": "/public/flutter_service_worker.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/public/icons/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 