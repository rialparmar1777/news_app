{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "build/web",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "framework": null
} 